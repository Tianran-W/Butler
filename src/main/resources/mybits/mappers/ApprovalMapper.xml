<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.inventory.approval.mapper.ApprovalMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="approvalId" keyColumn="approval_id">
        INSERT INTO tb_approval (
            material_id,
            user_id,
            approval_reason,
            approval_status,
            approval_time
        ) VALUES (
                     #{materialId},
                     #{userId},
                     #{approvalReason},
                     #{approvalStatus},
                     #{approvalTime}
                 )
    </insert>
</mapper>